/** Initializes a new Snake game and starts running it. */
class Main {
    function void main() {

        //
        // --- Variable declaration ---
        //
        var Game game;

        // --- INITIALISE GAME ---
        let game = Game.new();
        do game.run();

        // --- FINISH CONDITION ---
        do Output.println();
        do Output.printString("GAME OVER");
        return;
    }

}
